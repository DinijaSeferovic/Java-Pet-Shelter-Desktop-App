<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane fx:id="mainPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffffff;" stylesheets="@../css/mainscreen.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.controllers.UsersController">
   <center>
      <ScrollPane fx:id="scrollUsers" prefHeight="200.0" prefWidth="200.0" style="-fx-background: #ffffff;" styleClass="transparent" BorderPane.alignment="CENTER">
         <content>
            <GridPane fx:id="gridUsers" prefHeight="0.0" prefWidth="0.0">
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
         </content>
         <BorderPane.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </ScrollPane>
   </center>
   <top>
      <Label text="Overview of all users" textFill="#ff5757" textOverrun="CLIP">
         <font>
            <Font name="Century Gothic Bold" size="30.0" />
         </font>
         <padding>
            <Insets left="70.0" />
         </padding>
         <BorderPane.margin>
            <Insets left="70.0" top="30.0" />
         </BorderPane.margin>
      </Label>
   </top>
</BorderPane>
