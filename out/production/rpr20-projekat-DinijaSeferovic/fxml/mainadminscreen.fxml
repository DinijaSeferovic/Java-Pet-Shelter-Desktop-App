<BorderPane fx:id="mainPaneAdmin" prefHeight="575.0" prefWidth="875.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.controllers.MainAdminController">
   <stylesheets>
      <URL value="@../css/mainscreen.css" />
   </stylesheets>
